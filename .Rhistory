#Removing NA Values
comfort.level <-  comfort.level[!(is.na(comfort.level$Mental) | comfort.level$Physical==""), ]
#Filtering out Mental health data and counting it
yes.mental <- filter(comfort.level, grepl("Yes", Mental)) %>%
count() %>%
as.numeric()
